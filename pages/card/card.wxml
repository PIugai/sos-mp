<!--pages/card/card.wxml-->
<view class="navbar">
  <CustomNav >
    <view class='pair'>
      <view>
        <image bindtap="goBack" id='back' mode="aspectFit" src="/images/back.png" />
      </view>
      <image id='logo' mode="aspectFit" src="/images/logo.png" />
    </view>
  </CustomNav>
</view>

<view  class='card'>
  <view>{{user_nickname.nickname}}</view>
  <view>Capacity: {{capacity}}</view>
  <view>Description: {{description}}</view>
  <view>Address: {{location}}</view>
  <view>Contact: {{contact_number}}</view>
  <view>Full? {{is_full}}</view>
</view>


